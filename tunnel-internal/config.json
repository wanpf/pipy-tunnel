{
  "reverseServer": {
    "target": "127.0.0.1:8000",
    "idleTimeout": "300s",
    "tlsCert": "tls/server1.crt",
    "tlsKey": "tls/server1.key",
    "tlsCA": "tls/server1.ca"
  },
  "policies": {
    "connectRetry": 1
  },
  "global": {
    "enableDebug": false
  },
  "network": {
    "virtual_ip_addresses": [
      "127.0.0.111"
    ],
    "interval": "30s"
  }
}